{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "artesaos/seotools": "^0.22.0",
        "carbon-cli/carbon-cli": "^1.2",
        "fruitcake/laravel-cors": "^3.0.0",
        "guzzlehttp/guzzle": "^7.4.3",
        "jackiedo/timezonelist": "^5.1",
        "laravel/framework": "^v9.17.0",
        "laravel/helpers": "^1.5",
        "laravel/sanctum": "^v2.15.1",
        "laravel/tinker": "^2.7.2",
        "laravel/ui": "^3.4.6",
        "livewire/livewire": "^2.10.5",
        "menshinobi/laravel-fullcalendar": "^1.3",
        "munafio/chatify": "^1.5",
        "php-flasher/flasher-laravel": "^0.9.8",
        "php-flasher/flasher-sweet-alert-laravel": "^0.9.8",
        "pusher/pusher-php-server": "^7.0.2",
        "spatie/laravel-permission": "^5.5",
        "squizlabs/php_codesniffer": "3.6.2",
        "yajra/laravel-datatables-oracle": "^9.20.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12.3",
        "fakerphp/faker": "^v1.19.0",
        "friendsofphp/php-cs-fixer": "^3.8",
        "laravel/sail": "^1.14.10",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^6.2",
        "nunomaduro/phpinsights": "^2.4",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpunit/phpunit": "^9.5.20",
        "spatie/laravel-ignition": "^1.2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "treeware/plant": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
